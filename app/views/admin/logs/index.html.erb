<h3><%= t(:logs) %>&nbsp;&nbsp;<small><a href="/admin/dashboards/index"><%= t :back_to_dashboard %></a></small></h3>

<div class="panel panel-default">
    <!-- Table -->
    <table class="table table-striped table-bordered small">
        <thead>
            <tr>
              <th class="col-md-12"><%= t(:path) %></th>
            </tr>
        </thead>
        <tbody>
            <% @files.each do |f| %>
            <tr>
              <td><a href="/admin/logs/show?path=<%= f %>"><%= f %></a></td>
            </tr>
            <% end %>
        </tbody>
    </table>
</div>
