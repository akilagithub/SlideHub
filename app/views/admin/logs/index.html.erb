<h3><%= t(:logs) %>&nbsp;&nbsp;<small><%= link_to t(:back_to_dashboard), admin_dashboards_path %></small></h3>

<div class="panel panel-default">
  <!-- Table -->
  <table class="table table-striped table-bordered small">
    <thead>
      <tr>
        <th class="col-md-12"><%= t(:path) %></th>
      </tr>
    </thead>
    <tbody>
      <% @files.each do |f| %>
      <tr>
        <td><%= link_to f, "#{admin_logs_show_path}?path=#{f}" %></td>
      </tr>
      <% end %>
    </tbody>
  </table>
</div>
